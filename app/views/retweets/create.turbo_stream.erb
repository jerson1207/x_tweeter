<%= turbo_stream.replace dom_id(@tweet) do %>
  <%= render partial: "tweets/tweet", locals: { tweet_presenter: TweetPresenter.new(tweet: @tweet, current_user: current_user) } %>
<% end %>

<%= turbo_stream.replace dom_id(@tweet, :show) do %>
  <%= render partial: "tweets/action_links", locals: { tweet_presenter: TweetPresenter.new(tweet: @tweet, current_user: current_user) } %>
<% end %>

